{
  "cli": {
    "version": ">= 16.5.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENV": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "image": "latest",
        "secrets": {
          "googleServices": {
            "json": {
              "path": "google-services.json",
              "secret": "googleServicesJson"
            }
          }
        }
      },
      "env": {
        "ENV": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
